   "source": [
    "import numpy as np\n",
    "\n",
    "arr = np.array([1, 7, 3, 9, 4, 8])\n",
    "\n",
    "# value >= 4 in arr \n",
    "out = np.where(arr >= 4)\n",
    "print(out)\n",
    "\n",
    "# Value\n",
    "arr = arr[out[0]]\n",
    "print(arr)"
   ]
  }
